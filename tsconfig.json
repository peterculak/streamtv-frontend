{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true, // false causes this error Type error: Duplicate identifier 'LibraryManagedAttributes'.  TS2300 (possibly typescript bug)
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "lib": ["dom", "es6"]
  },
  "include": [
    "src"
  ]
}
